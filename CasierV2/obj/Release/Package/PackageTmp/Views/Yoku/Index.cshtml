    <script>

        var images = new Array('/Image/maki.jpg', '/Image/japanese-food-in-lincoln-park.jpg', '/Image/plated_sushi.jpg');
        var nextimage = 0;

        function doSlideshow() {

            if (nextimage >= images.length) { nextimage = 0; }
            $('#headerwrap')
                .css('background-image', 'url("' + images[nextimage++] + '")')
                .fadeIn("5000", function () {
                    setTimeout(doSlideshow, 5000);
                });


        }

        $(document).ready(function () {

            var iOS = /iPad|iPhone|iPod/.test(navigator.userAgent) && !window.MSStream;

            if (iOS) {
                $("#headerSection").hide();
            }

            doSlideshow();

            $('nav a').on('click', function () {

                var scrollAnchor = $(this).attr('data-scroll'),
                    scrollPoint = $('section[data-scroll^="' + scrollAnchor + '"]').offset().top - 50;

                $('body,html').animate({
                    scrollTop: scrollPoint
                }, 500);

                return false;

            });


        });
        window.onscroll = function () {
            if (window.pageYOffset >= 200) {
                jQuery('.header-content').css({ display: 'none' });

            }
            else {
                jQuery('.header-content').css({ display: 'block' });

            }
        }

        function toggleInfo() {


            $("#info").toggle('slow');


            setTimeout(function () {

                if ($('#info').is(':visible')) {
                    $("#BusinessName").text("Close Map");
                }
                else {
                    $("#BusinessName").text("Kontakt os");
                }

            }, 600);
        }

    </script>

